@PACKAGE_INIT@

set(LIBLANGINFO_VERSION       "@PROJECT_VERSION@")
set(LIBLANGINFO_VERSION_MAJOR "@PROJECT_VERSION_MAJOR@")
set(LIBLANGINFO_VERSION_MINOR "@PROJECT_VERSION_MINOR@")
set(LIBLANGINFO_VERSION_PATCH "@PROJECT_VERSION_PATCH@")

set_and_check(LIBLANGINFO_INSTALL_DIR "${PACKAGE_PREFIX_DIR}")
set_and_check(LIBLANGINFO_INCLUDE_DIR "${PACKAGE_PREFIX_DIR}/include")
set_and_check(LIBLANGINFO_LIBRARY_DIR "${PACKAGE_PREFIX_DIR}/lib")

include("${CMAKE_CURRENT_LIST_DIR}/liblanginfo-staticTargets.cmake")
include("${CMAKE_CURRENT_LIST_DIR}/liblanginfo-sharedTargets.cmake")

set(LIBLANGINFO_LIBRARY_SHARED liblanginfo::shared)
set(LIBLANGINFO_LIBRARY_STATIC liblanginfo::static)
set(LIBLANGINFO_LIBRARY        liblanginfo::static)
